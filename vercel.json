{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install --frozen-lockfile",
  "routes": [
    {
      "src": "/api/:path*",
      "dest": "/api/:path*"
    },
    {
      "src": "/servicios",
      "status": 410
    },
    {
      "src": "/contacto",
      "status": 410
    },
    {
      "src": "/proyectos",
      "status": 410
    },
    {
      "src": "/acerca-de",
      "status": 410
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Frame-Options": "SAMEORIGIN",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "strict-origin-when-cross-origin"
      }
    }
  ]
}
